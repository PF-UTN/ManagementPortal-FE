{
  "name": "management-portal-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --configuration development",
    "prod": "ng serve --configuration production",
    "start": "ng serve",
    "build:dev": "ng build --configuration development",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci",
    "lint": "npx eslint --fix",
    "eslint:ci": "npx eslint . -f json -o lint/eslint_report.json",
    "format": "prettier --write \"./**/*.ts\"",
    "generate-lib": "node scripts/generate-lib.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.0.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "18.0.0",
    "@angular/compiler": "18.0.0",
    "@angular/core": "18.0.0",
    "@angular/forms": "18.0.0",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "18.0.0",
    "@angular/platform-browser-dynamic": "18.0.0",
    "@angular/router": "18.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "bootstrap": "^5.3.3",
    "rxjs": "~7.8.0",
    "strip-json-comments": "^5.0.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.0",
    "@eslint/js": "^9.21.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.0",
    "lint-staged": "^15.4.3",
    "ng-packagr": "^18.2.0",
    "prettier": "^3.5.2",
    "punycode": "^2.3.1",
    "ts-jest": "^29.2.5",
    "typescript": "~5.5.2",
    "typescript-eslint": "^8.24.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,scss,md}": [
      "npm run lint",
      "prettier --write"
    ]
  }
}
